{
    "cli": {
        "version": ">= 5.0.0",
        "appVersionSource": "local"
    },
    "build": {
        "preview": {
            "android": {
                "buildType": "apk",
                "credentialsSource": "remote"
            }
        },
        "production": {
            "android": {
                "buildType": "app-bundle",
                "credentialsSource": "remote"
            }
        }
    },
    "submit": {
        "production": {
            "android": {
                "serviceAccountKeyPath": "./google-services.json",
                "track": "internal"
            }
        }
    }
}